# Database Configuration
POSTGRES_USER="postgres"
POSTGRES_PASSWORD="postgres"
POSTGRES_DB="postgres"
DB_USER="postgres"
DB_PASSWORD="postgres"
DB_NAME="postgres"
DATABASE_URL="postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgres:5432/${POSTGRES_DB}"

# AWS Configuration
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""
AWS_REGION_NAME="us-east-1"

# LiteLLM Settings
LITELLM_MASTER_KEY="sk-litellm-test-key"
OPENAI_API_BASE="/v1"
SERVE_OPENAI_SWAGGER=true
UI_USERNAME="litellm"
UI_PASSWORD="litellm"
STORE_MODEL_IN_DB=true
CONFIG_FILE="iam_role_config.yml"

# MCP Settings
MCP_LOG_LEVEL="debug"
MCP_TIMEOUT=60

# Context7 MCP Settings
CONTEXT7_MCP_CACHE_DIR="/app/.cache/context7"
CONTEXT7_MCP_MAX_TOKENS=5000

# AWS Documentation MCP Settings
AWS_DOCS_MCP_CACHE_DIR="/app/.cache/aws-docs"
AWS_DOCS_MCP_MAX_LENGTH=5000
